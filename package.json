{
  "name": "torrz",
  "productName": "Torrz",
  "version": "0.0.0-alpha.1",
  "description": "HTML 5 Elegant torrent client for all users",
  "author": "Arilson Bolivar (Nexasoft)",
  "homepage": "https://github.com/nexasoft/Torrz#readme",
  "license": "GPL-3.0-or-later",
  "main": "torrz.js",
  "scripts": {
    "start": "electron --enable-transparent-visuals .",
    "package-mac": "electron-packager . torrz --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=build --app-version=0.0.0-alpha.1 --version-string.CompanyName=Nexasoft --version-string.FileDescription=Nexa --version-string.ProductVersion=0.0.0-alpha.1 --version-string.ProductName='Torrz' --app-bundle-id='ml.nexasoft.torrz'",
    "package-win": "electron-packager . torrz --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=build --app-version=0.0.0-alpha.1 --version-string.CompanyName=Nexasoft --version-string.FileDescription=Nexa --version-string.ProductVersion=0.0.0-alpha.1 --version-string.ProductName='Torrz' --app-bundle-id='ml.nexasoft.torrz'",
    "package-linux": "--asar=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexasoft/Torrz.git"
  },
  "keywords": [
    "torrz",
    "torrent",
    "client",
    "nexa"
  ],
  "bugs": {
    "url": "https://github.com/nexasoft/Torrz/issues"
  },
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1",
    "npm": "^6.9.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "webtorrent": "^0.103.1",
    "connect": "^3.6.6",
    "jquery": ">=3.5.0",
    "npm": "^6.9.0",
    "serve-static": "^1.13.2"
  }
}
